{
  "name": "brain-station-backend",
  "version": "1.0.0",
  "description": "Backend for BrainStation learning application",
  "main": "dist/app.js",
  "scripts": {
    "build": "npm run clean && npm run build-babel && npm run copy",
    "build-babel": "babel -d ./dist ./src -s",
    "clean": "rm -rf dist && mkdir dist",
    "copy": "babel src --out-dir ./dist --copy-files",
    "dev": "nodemon --exec babel-node src/app.js",
    "format": "prettier --write --cache \"**/*.{js,md}\"",
    "lint": "eslint src --ext js,md --ignore-path .gitignore --fix --cache --report-unused-disable-directives",
    "prepare": "husky",
    "start": "node dist/app.js",
    "test": "echo test"
  },
  "keywords": [],
  "author": "Danuja Jayasuriya, Supun Kariyawasam, Oshada Thawalampola, Praweena Makawita",
  "license": "ISC",
  "dependencies": {
    "@sliit-foss/functions": "2.10.0",
    "@sliit-foss/http-logger": "1.3.1",
    "@sliit-foss/module-logger": "1.3.1",
    "@tensorflow/tfjs": "4.21.0",
    "axios": "1.7.7",
    "bcryptjs": "2.4.3",
    "brain-station-backend": "file:",
    "celebrate": "15.0.3",
    "compression": "1.7.4",
    "cors": "2.8.5",
    "express": "4.19.2",
    "express-http-context": "1.2.4",
    "express-rate-limit": "7.4.0",
    "firebase": "10.14.1",
    "firebase-admin": "12.4.0",
    "form-data": "4.0.0",
    "handlebars": "4.7.8",
    "helmet": "7.1.0",
    "http-errors": "2.0.0",
    "jsonwebtoken": "9.0.2",
    "lodash": "4.17.21",
    "moment": "2.30.1",
    "mongoose": "8.6.0",
    "mongoose-aggregate-paginate-v2": "1.1.2",
    "mongoose-paginate-v2": "1.8.3",
    "multer": "1.4.5-lts.1",
    "nodemailer": "6.9.14",
    "openai": "4.57.0",
    "readline": "1.3.0",
    "uuid": "10.0.0"
  },
  "devDependencies": {
    "@babel/cli": "7.25.6",
    "@babel/core": "7.25.2",
    "@babel/eslint-parser": "7.25.1",
    "@babel/node": "7.25.0",
    "@babel/preset-env": "7.25.4",
    "@commitlint/cli": "19.4.1",
    "@commitlint/config-conventional": "19.4.1",
    "@sliit-foss/eslint-config-internal": "1.1.6",
    "@trivago/prettier-plugin-sort-imports": "4.3.0",
    "babel-plugin-module-resolver": "5.0.2",
    "dotenv": "16.4.5",
    "eslint": "8.57.0",
    "eslint-plugin-import": "2.29.1",
    "husky": "9.1.5",
    "kill-port": "2.0.1",
    "nodemon": "3.1.4",
    "prettier": "3.3.3"
  }
}
